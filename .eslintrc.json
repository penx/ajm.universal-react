{
  "parser": "babel-eslint",
  "extends": "airbnb",
  "plugins": [
    "flowtype"
  ],
  "env": {
    "es6": true
  },
  "rules": {
    "semi": [2, "never"],
    "prefer-arrow-callback": 0,
    "comma-dangle": 0,
    "react/jsx-filename-extension": 0,
    "react/prefer-stateless-function": 0,
    "react/forbid-prop-types": 0,
    "react/jsx-closing-tag-location": 0
  }
}
